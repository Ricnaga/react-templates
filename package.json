{
  "name": "react-templates",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@chakra-ui/icons": "^2.0.17",
    "@chakra-ui/react": "^2.5.1",
    "@emotion/react": "^11.10.5",
    "@emotion/styled": "^11.10.5",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/jest": "^29.4.0",
    "axios": "^1.3.3",
    "formik": "^2.2.9",
    "framer-motion": "^9.0.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.8.1",
    "react-scripts": "5.0.1",
    "ts-jest": "^29.0.5",
    "web-vitals": "^2.1.0"
  },
  "scripts": {
    "start": "cross-env TS_NODE_PROJECT=\".config/webpack.tsconfig.json\" webpack serve --config .config/webpack.dev.ts",
    "build": "cross-env TS_NODE_PROJECT=\".config/webpack.tsconfig.json\" webpack --config .config/webpack.prod.ts",
    "lint": "eslint --ext .ts --ext .tsx src",
    "lint:fix": "yarn lint --fix",
    "precommit": "lint-staged",
    "prepare": "husky install",
    "typecheck": "tsc -p tsconfig.json --pretty --noEmit",
    "mock": "yarn json-server -p 3333 -d 1250 src/application/api/mocks/json-server/db.json",
    "dev": "concurrently \"yarn start\" \"yarn mock\"",
    "clean": "rimraf node_modules",
    "commit": "git-cz",
    "test": "jest"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/cli": "^7.20.7",
    "@babel/core": "^7.20.12",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-react": "^7.18.6",
    "@babel/preset-typescript": "^7.18.6",
    "@babel/register": "^7.18.9",
    "@commitlint/cli": "^17.4.3",
    "@commitlint/config-conventional": "^17.4.3",
    "@types/dotenv-webpack": "^7.0.3",
    "@types/node": "^18.13.0",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.10",
    "@types/react-router-dom": "^5.3.3",
    "@types/webpack": "^5.28.0",
    "@typescript-eslint/eslint-plugin": "^5.52.0",
    "@typescript-eslint/parser": "^5.52.0",
    "babel-loader": "^9.1.2",
    "clean-webpack-plugin": "^4.0.0",
    "commitizen": "^4.3.0",
    "compression-webpack-plugin": "^10.0.0",
    "concurrently": "^7.6.0",
    "cross-env": "^7.0.3",
    "css-loader": "^6.7.3",
    "cz-conventional-changelog": "^3.3.0",
    "dotenv-webpack": "^8.0.1",
    "eslint": "^8.34.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^8.6.0",
    "eslint-import-resolver-typescript": "^3.5.3",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.5.0",
    "husky": "^8.0.3",
    "jest": "^29.4.3",
    "jest-environment-jsdom": "^29.4.3",
    "json-server": "^0.17.1",
    "lint-staged": "^13.1.2",
    "mini-css-extract-plugin": "^2.7.2",
    "prettier": "^2.8.4",
    "rimraf": "^4.1.2",
    "style-loader": "^3.3.1",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.1.2",
    "tsconfig-paths-webpack-plugin": "^4.0.0",
    "typescript": "^4.9.5",
    "webpack": "^5.75.0",
    "webpack-cli": "^5.0.1",
    "webpack-dev-server": "^4.11.1",
    "webpack-merge": "^5.8.0"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog",
      "disableScopeLowerCase": true,
      "disableSubjectLowerCase": true
    }
  }
}
