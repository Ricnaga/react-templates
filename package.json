{
  "name": "react-templates",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@chakra-ui/icons": "^2.0.19",
    "@chakra-ui/react": "^2.6.1",
    "@emotion/react": "^11.11.0",
    "@emotion/styled": "^11.11.0",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/jest": "^29.5.1",
    "axios": "^1.4.0",
    "formik": "^2.2.9",
    "framer-motion": "^10.12.16",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.11.2",
    "react-scripts": "^5.0.1",
    "ts-jest": "^29.1.0",
    "web-vitals": "^3.3.1"
  },
  "scripts": {
    "start": "cross-env TS_NODE_PROJECT=\".config/webpack.tsconfig.json\" webpack serve --config .config/webpack.dev.ts",
    "build": "cross-env TS_NODE_PROJECT=\".config/webpack.tsconfig.json\" webpack --config .config/webpack.prod.ts",
    "lint": "eslint --ext .ts --ext .tsx src",
    "lint:fix": "yarn lint --fix",
    "precommit": "lint-staged",
    "prepare": "husky install",
    "typecheck": "tsc -p tsconfig.json --pretty --noEmit",
    "mock": "yarn json-server -p 3333 -d 1250 src/application/api/mocks/json-server/db.json",
    "dev": "concurrently \"yarn start\" \"yarn mock\"",
    "clean": "rimraf node_modules",
    "commit": "git-cz",
    "test": "jest",
    "update:latest": "yarn upgrade --latest"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/cli": "^7.21.5",
    "@babel/core": "^7.21.8",
    "@babel/preset-env": "^7.21.5",
    "@babel/preset-react": "^7.18.6",
    "@babel/preset-typescript": "^7.21.5",
    "@babel/register": "^7.21.0",
    "@commitlint/cli": "^17.6.3",
    "@commitlint/config-conventional": "^17.6.3",
    "@types/dotenv-webpack": "^7.0.3",
    "@types/node": "^20.2.3",
    "@types/react": "^18.2.6",
    "@types/react-dom": "^18.2.4",
    "@types/react-router-dom": "^5.3.3",
    "@types/webpack": "^5.28.1",
    "@typescript-eslint/eslint-plugin": "^5.59.7",
    "@typescript-eslint/parser": "^5.59.7",
    "babel-loader": "^9.1.2",
    "clean-webpack-plugin": "^4.0.0",
    "commitizen": "^4.3.0",
    "compression-webpack-plugin": "^10.0.0",
    "concurrently": "^8.0.1",
    "cross-env": "^7.0.3",
    "css-loader": "^6.7.4",
    "cz-conventional-changelog": "^3.3.0",
    "dotenv-webpack": "^8.0.1",
    "eslint": "^8.41.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^8.8.0",
    "eslint-import-resolver-typescript": "^3.5.5",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.5.1",
    "husky": "^8.0.3",
    "jest": "^29.5.0",
    "jest-environment-jsdom": "^29.5.0",
    "json-server": "^0.17.3",
    "lint-staged": "^13.2.2",
    "mini-css-extract-plugin": "^2.7.6",
    "prettier": "^2.8.8",
    "rimraf": "^5.0.1",
    "style-loader": "^3.3.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "tsconfig-paths-webpack-plugin": "^4.0.1",
    "typescript": "^5.0.4",
    "webpack": "^5.83.1",
